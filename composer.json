{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/annotations": "^1.0",
        "doctrine/dbal": "^2.13",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^2.2",
        "doctrine/orm": "^2.10",
        "phpdocumentor/reflection-docblock": "^4.3",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "^4.4.1",
        "symfony/console": "^4.4.1",
        "symfony/dotenv": "^4.4.1",
        "symfony/expression-language": "^4.4.1",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^4.4.1",
        "symfony/http-client": "^4.4.1",
        "symfony/intl": "^4.4.1",
        "symfony/property-access": "^4.4.1",
        "symfony/property-info": "^4.4.1",
        "symfony/proxy-manager-bridge": "^4.4.1",
        "symfony/security-bundle": "^4.4.1",
        "symfony/serializer": "^4.4.1",
        "symfony/twig-bundle": "^4.4.1",
        "symfony/validator": "^4.4.1",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/yaml": "^4.4.1",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/browser-kit": "^4.4.1",
        "symfony/css-selector": "^4.4.1",
        "symfony/debug-bundle": "^4.4.1",
        "symfony/maker-bundle": "^1.34",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/stopwatch": "^4.4.1",
        "symfony/web-profiler-bundle": "^4.4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.4.1"
        }
    }
}
